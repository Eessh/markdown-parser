# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  MarkdownParser_config = debug

else ifeq ($(config),release)
  MarkdownParser_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := MarkdownParser

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

MarkdownParser:
ifneq (,$(MarkdownParser_config))
	@echo "==== Building MarkdownParser ($(MarkdownParser_config)) ===="
	@${MAKE} --no-print-directory -C . -f MarkdownParser.make config=$(MarkdownParser_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f MarkdownParser.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   MarkdownParser"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"