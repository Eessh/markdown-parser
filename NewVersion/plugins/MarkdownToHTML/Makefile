# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Plugin__Markdown_to_HTML_config = debug

else ifeq ($(config),release)
  Plugin__Markdown_to_HTML_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := Plugin__Markdown_to_HTML

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Plugin__Markdown_to_HTML:
ifneq (,$(Plugin__Markdown_to_HTML_config))
	@echo "==== Building Plugin__Markdown_to_HTML ($(Plugin__Markdown_to_HTML_config)) ===="
	@${MAKE} --no-print-directory -C . -f Plugin__Markdown_to_HTML.make config=$(Plugin__Markdown_to_HTML_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Plugin__Markdown_to_HTML.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Plugin__Markdown_to_HTML"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"